    # 4.3 requires this
    chmod 0644 /proc/cmdline

    setprop wifi.interface wlan0

    setprop ro.telephony.call_ring.multiple false

    #Set SUID bit for usbhub
# LS4 USB[+]
# usbhub must not have super user permission. It causes fail for CTS test.
